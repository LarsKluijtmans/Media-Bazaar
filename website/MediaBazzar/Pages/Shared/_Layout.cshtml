<!DOCTYPE html>
<html lang="en">
<head>
    <title>@ViewData["Title"] - MediaBazzar</title>
    @*<link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />*@
    <link rel="stylesheet" href="~/css/view.css" />
</head>
<body>
    <div class="web-body">
        <div class="header">
            <label class="logo">MediaBazaar</label>
            <nav>
                <a class="home-button"asp-area="" asp-page="/Index">Home</a><br>

                <a class="login-button" asp-area="" asp-page="/Login">Login</a>
            </nav>
        </div>


        <div>
           @RenderBody()
        </div>

        <div class="footer">
            <a class="a nav-link text-dark" asp-area="" asp-page="/">Contact us</a>
        </div>


        @RenderSection("Scripts", required: false)
    </div>
</body>
</html>
