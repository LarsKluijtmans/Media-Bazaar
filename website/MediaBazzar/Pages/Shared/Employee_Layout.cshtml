<!DOCTYPE html>
<html lang="en">
<head>
    <title>@ViewData["Title"] - MediaBazzar</title>
    @*<link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />*@
    <link rel="stylesheet" href="~/css/view.css" />
</head>
<body>
    <div  class="web-body">
        <div class="header">
            <nav>
                <a class="a  text-dark" asp-area="" asp-page="/EmployeeHome">Home</a>
                <a class="a text-dark" asp-area="" asp-page="/EmployeeViewinfo">View Profile</a>
                <a class="a text-dark" asp-area="" asp-page="/EmployeeViewSchedule">View Schedule</a>
